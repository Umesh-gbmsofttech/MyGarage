{
  "expo": {
    "name": "MyGarage",
    "slug": "MyGarageApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/MyGarage.jpg",
    "scheme": "mygarageapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/MyGarage.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_EXTERNAL_STORAGE"
      ],
      "package": "com.mygarage.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/MyGarage.jpg"
    },
    "extra": {
      "buildNumber": 10,
      "apiBaseUrl": "https://mygarage-server.onrender.com/api"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker",
      "@maplibre/maplibre-react-native",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/MyGarage.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
